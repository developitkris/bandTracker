@{
  Layout = "_Layout";
}
<h3>@Model["venue"].GetVenueName()</h3>
<div class="container container-fluid">
  <div class="row1">
    <div class="col-md-12">
      <p>Venue Id</p>
      <p>Venue Name</p>
    </div>
  </div>
  @foreach(var band in Model["bands"])
  {
    <div class="row2">
      <div class="col-md-12">
        <p>@band.GetId()</p>
        <p>@band.GetBandName()</p>
      </div>
    </div>
  }
</div>
